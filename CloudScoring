// A mapping from a common name to the sensor-specific bands.
var LC8_BANDS = ['B2',   'B3',    'B4',  'B5',  'B6',    'B7',    'B10'];
var STD_NAMES = ['blue', 'green', 'red', 'nir', 'swir1', 'swir2', 'temp'];

// Get the Landsat 8 TOA collection
var lsat = ee.ImageCollection('LC8_L1T_TOA')
    .filterDate('2015-07-10', '2015-09-15');

print(lsat);
print(lsat.mosaic())

var cloudScored = ee.Algorithms.Landsat.simpleCloudScore(lsat);
print(cloudScored);

var vizParams = {'bands': ['B6', 'B5', 'B7'], 'max': .8, 'gamma': 1.6};

// Draw to map
Map.addLayer(lsat.mosaic(), vizParams);
Map.addLayer(cloudScored);