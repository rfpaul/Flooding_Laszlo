// Apply a mask value for NaN at the area of interest
var maskAndFill = function(image, aoi, nan_val) {
  var mask = ee.Image(0).byte().paint(aoi, 1);
  var fill = mask.not().multiply(nan_val);

  return image.multiply(mask).add(fill);
}

